<nav class="navbar navbar-expand-lg navbar-light align-items-center">
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <div class="navbar-nav">
      <a class="nav-item nav-link active" href="#">Home</a>
      <a class="nav-item nav-link" href="/saved">Saved Articles</a>
    </div>
  </div>          
  <a class="btn btn-sm btn-danger scrape-new">Scrape Articles</a>
</nav>
<div class="jumbotron text-center">
  <div class="blog_entry__title">
  </div>
  <div class="blog_entry__publish_info">
  </div>
  <div class="blog_entry__teaser">
    <h1>Psych Scraper</h1>
    <p>Psychology Todat</p>
  </div>
</div>
<div class="container-fluid article-container">  
  {{#each data}}
    <div class='card'>
      <div class='card-header'>
        <div class="row align-items-center">
          <div class="col-sm-10">
            <h3>
              <a class='article-link' target='_blank' href= {{this.link}} >
                {{this.title}} 
              </a>
            </h3> 
          </div>
          <div class="col-sm-2">
              <a id={{this._id}} class='btn btn-success btn-sm save'>
                Save Article
              </a>
          </div> 
        </div>       
      </div>
      <div class='card-body'>
        {{this.summary}}
      </div>
    </div>
  {{/each}}
</div>